{
  "name": "project-web",
  "description": "React frontend for the template project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npm --prefix ../shared i",
    "update:browserlist": "npx browserslist@latest --update-db",
    "ionic:build": "npm i && craco build",
    "ionic:serve": "BROWSER=none craco start",
    "build": "ionic build",
    "dev": "ionic serve",
    "deploy:dev": "ionic cap sync && ntl deploy",
    "deploy:prod": "ionic cap sync && ntl deploy --prod",
    "hooks:update": "lefthook install -a",
    "ios": "ionic cap sync ios && ionic cap run ios --no-build",
    "ios:open": "ionic cap sync ios && ionic cap open ios",
    "ios:live": "ionic cap sync ios && ionic cap run ios -l --external",
    "android": "ionic cap sync android && ionic cap run android --no-build",
    "android:open": "ionic cap sync android && ionic cap open android",
    "android:live": "ionic cap sync android && ionic cap run android -l --external"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@capacitor/android": "^3.3.2",
    "@capacitor/app": "1.0.2",
    "@capacitor/clipboard": "^1.0.7",
    "@capacitor/core": "3.3.2",
    "@capacitor/haptics": "1.0.2",
    "@capacitor/ios": "^3.3.2",
    "@capacitor/keyboard": "1.0.2",
    "@capacitor/network": "^1.0.7",
    "@capacitor/push-notifications": "^1.0.7",
    "@capacitor/status-bar": "1.0.2",
    "@headlessui/react": "^1.4.1",
    "project-shared": "file:../shared",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/onesignal": "^5.36.0",
    "@ionic/pwa-elements": "^3.0.2",
    "@ionic/react": "^5.5.0",
    "@ionic/react-router": "^5.5.0",
    "@react-hook/media-query": "^1.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.6.3",
    "parse": "^3.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-map-gl": "^6.1.16",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "tailwindcss": "^2.2.4",
    "twind": "^0.16.16",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^6.3.0",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-range-requests": "^6.3.0",
    "workbox-routing": "^6.3.0",
    "workbox-strategies": "^6.3.0",
    "workbox-streams": "^6.3.0",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@capacitor/cli": "3.3.1",
    "@craco/craco": "^6.4.3",
    "@ionic/cli": "^6.18.1",
    "@netlify/plugin-lighthouse": "^2.1.2",
    "@types/node": "^12.19.15",
    "@types/parse": "^2.18.13",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "netlify-cli": "^6.3.5",
    "prettier": "^2.3.2",
    "ts-loader": "^8.3.0",
    "typescript": "^4.1.3"
  }
}
